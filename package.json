{
  "name": "stowryjs",
  "version": "0.1.0",
  "type": "module",
  "description": "JavaScript/TypeScript SDK for Stowry presigned URLs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest run && vitest run --config vitest.browser.config.ts",
    "test:node": "vitest run",
    "test:browser": "vitest run --config vitest.browser.config.ts",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "stowry",
    "presigned",
    "s3",
    "storage"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sagarc03/stowryjs.git"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@vitest/browser": "^4.0.17",
    "@vitest/browser-playwright": "^4.0.17",
    "@vitest/coverage-v8": "^4.0.17",
    "playwright": "^1.57.0",
    "tsdown": "^0.20.0-beta.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
